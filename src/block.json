{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 2,
    "name": "croquet/webshowcase-block",
    "version": "0.1.0",
    "title": "Croquet Metaverse Web Showcase",
    "category": "text",
    "icon": "flag",
    "description": "Croquet Web Showcase",
    "attributes": {
	"message": {
	    "type": "string",
	    "source": "text",
	    "selector": "div"
	}
