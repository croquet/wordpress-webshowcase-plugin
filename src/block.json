{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 2,
    "name": "croquet/webshowcase-block",
    "version": "0.1.0",
    "title": "Croquet Metaverse Web Showcase",
    "category": "text",
    "icon": "flag",
    "description": "Croquet Web Showcase",
    "attributes": {
        "cardsString": {
            "type": "string",
            "selector": "div",
            "default": "[{\"place\":1,\"type\":\"image\",\"path\":\"https://croquet.io/webshowcase/site/CompanyLogo.jpg\"},{\"place\":2,\"type\":\"pdf\",\"path\":\"https://croquet.io/webshowcase/site/CompanyDeck.pdf\"}]"
        },
        "minHeight": {
            "type": "string",
            "selector": "div",
            "default": "800px"
        }
    },
    "supports": {
        "html": false
    },
    "textdomain": "croquet",
    "editorScript": "file:./index.js",
    "editorStyle": "file:./index.css",
    "style": "file:./style-index.css"
}
