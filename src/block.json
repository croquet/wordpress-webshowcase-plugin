    "supports": {
	"html": false
    },
    "textdomain": "croquet",
    "editorScript": "file:./index.js",
    "editorStyle": "file:./index.css",
    "style": "file:./style-index.css",
    "renderCallback": "webshowcase_dynamic_render_callback",
    "render_callback": "webshowcase_dynamic_render_callback"
}
